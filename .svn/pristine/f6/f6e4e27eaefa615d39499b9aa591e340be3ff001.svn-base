<?xml version="1.0" encoding="utf-8"?>
<project outputprefix="rtc">

    <!--  Project description -->
    <name>Hodiny realneho casu (RTC)</name>
    <category>Demo aplikace</category>
    <author>Josef Strnadel</author>
    <authoremail>strnadel@fit.vutbr.cz</authoremail>
    <revision>20090201</revision>
    <description>Aplikace demonstruje implementaci RTC na platforme FITkit. 
       Pro mereni casu je vyuzit casovac pracujici v rezimu vystupni komparace. 
       Cas je aktualizovan v obsluze preruseni.
    </description>
 
    <!--  MCU part -->
    <mcu>
        <include>mcu/libs/lcd/package.xml</include>
        <file>user_defined.c</file>
        <file>main.c</file>
    </mcu>

    <!-- FPGA part -->
    <fpga use="apps/demo/display/project.xml" />

</project>
