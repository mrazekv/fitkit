<?xml version="1.0" encoding="utf-8"?>
<project outputprefix="signal_generator">

    <!--  Project description -->
    <name>Generátor signálů s nastavitelným kmitočtem a velikostí amplitudy napětí</name>
    <author>Jan Křepský (jádro aplikace), Josef Strnadel (rozšíření a dokumentace)</author>
    <authoremail>xkreps00@stud.fit.vutbr.cz; strnadel@fit.vutbr.cz</authoremail>
    <revision>20090417</revision>
    <description>
			Pomocí DA převodníku dostupného na mikrokontroléru aplikace demonstruje jednoduchý způsob generování základních průběhů signálů (konstantní, harmonický, pilovitý, obdélníkový) s možností nastavení kmitočtu a amplitudy zvoleného průběhu.
		</description>
 
    <!--  MCU part -->
    <mcu>
        <include>mcu/libs/lcd/package.xml</include>
        <file>main.c</file>
    </mcu>

    <!-- FPGA part -->
    <fpga use="apps/demo/display/project.xml" />
    
</project>
